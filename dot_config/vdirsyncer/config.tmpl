[general]
status_path = "~/.vdirsyncer/status/"

[pair calendars]
conflict_resolution = "b wins"
a = "calendars_local"
b = "calendars_dav"
collections = ["from b"]
metadata = ["color", "displayname"]

[storage calendars_local]
type = "filesystem"
path = "~/.calendars/"
fileext = ".ics"

[storage calendars_dav]
type = "caldav"

# We can simplify this URL here as well. In theory it shouldn't matter.
url = "{{ keepassxcAttribute "trezor/caldav" "main" }}"
username = "{{ (keepassxc "trezor/caldav").UserName }}"
password = "{{ (keepassxc "trezor/caldav").Password }}"
